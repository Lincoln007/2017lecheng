    <script type="text/javascript">
        layui.config({
            base: '/Scripts'
        }).extend({
            elem: '/jqmodules/elem',
            tabmenu: '/jqmodules/tabmenu'
        }).use('/index');

        layui.use(['layer'], function () {
            var layer = layui.layer;
            //layer.msg("1111111");

            $("#logout").click(function () {
                layer.msg('确定要退出吗？', {
                    time: 0 //不自动关闭
                  , btn: ['确认', '取消']
                  , yes: function (index) {
                      layer.close(index);
                      $.post('@Url.Content("~/Main/Logout")', {}, function (data) {
                          if (data.State == 1) {
                              window.location.href = data.URL;
                          } else {
                              layer.msg(data.Msg);
                          }
                      })
                  }
                });
            })

        });
    </script>
