<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<xml bitmap="2" bgcolor="#B9E1E1" fgcolor="#ec136c">
    @{
        var list = ViewData["explist"] as List<DBModel.DBmodel.base_exp_comp>;
    }
    <text mode="1" x="1" y="1" bgcolor="#006070" fgcolor="white" size="1" name="DrawBelt" width="26">-乐诚国际电商条码系统--</text>
    <text mode="1" size="1" x="2" y="3" fgcolor="black">ID:</text>
    <text mode="1" size="1" x="5" y="3" fgcolor="black">@ViewData["userid"]</text>
    <text mode="1" size="1" x="1" y="4" adjusty="4" fgcolor="black">包裹号:</text>
    <input bgcolor="#FFFFFF" mode="1" size="1" x="10" y="4" adjusty="4" width="14" param="packgecode" char="2" len="20" method="2"></input>
    <text mode="1" size="1" x="1" y="6" adjusty="7" fgcolor="black">快递类型:</text>
    <select default="2" size="1" x="10" starty="6" adjusty="5" number="4" pagesize="7" style="1" fgcolor="black" bgcolor="white" member="1" width="14">
        @foreach (var item in list)
          {
           <option x="1" bgcolor="white" url="ConfirmExpress?expid=@item.express_id">@item.express_name</option>
          }
    </select>
    <button mode="1" size="1" x="1" y="19" url="/MainMenu" key="a" func="1" value="" bgcolor="forestgreen" fgcolor="khaki" adjusty="2">[取消]</button>
    @*<button mode="1" size="1" x="17" y="19" url="/ConfirmDistrbuilt" key="b" func="2" value="" bgcolor="forestgreen" fgcolor="khaki" adjusty="2">[确定]</button>*@
</xml>
@{
    Layout = null;
}
