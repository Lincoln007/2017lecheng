<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="ThemeBucket">
    <link rel="shortcut icon" href="/images/title.ico" type="/images/png"><!--标题图标-->
    <link href="/Scripts/bootstrap/iCheck/skins/square/red.css" rel="stylesheet">
    <link href="/Scripts/bootstrap/iCheck/skins/square/square.css" rel="stylesheet">
    <title>@ViewBag.Title</title>

    <link href="/css/style.css" rel="stylesheet">
    <link href="/css/style-responsive.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="/css/layui.css" media="all">
    <!-- Placed js at the end of the document so the pages load faster -->
    <script src="/Scripts/jquery-1.10.2.min.js"></script>
    <script src="/Scripts/bootstrap/jquery-ui-1.9.2.custom.min.js"></script>
    <script src="/Scripts/bootstrap/jquery-migrate-1.2.1.min.js"></script>
    <script src="/Scripts/bootstrap/bootstrap.min.js"></script>
    <script src="/Scripts/bootstrap/modernizr.min.js"></script>
    <script src="/Scripts/bootstrap/jquery.nicescroll.js"></script>


    <!--common scripts for all pages-->
    <script src="/Scripts/bootstrap/scripts.js"></script>
    <script type="text/javascript" src="~/Scripts/layui/layui.js"></script>

    <style type="text/css">
        .mycheck {
            margin-right: 60px;
        }
    </style>

    @RenderSection("head", required: true)
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="js/html5shiv.js"></script>
    <script src="js/respond.min.js"></script>
    <![endif]-->
</head>

<body>
    <div class="main-content">@RenderBody()</div>


</body>
</html>
